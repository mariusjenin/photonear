GL_ARB_texture_storage
http://www.opengl.org/registry/specs/ARB/texture_storage.txt
GL_ARB_texture_storage
	GL_TEXTURE_IMMUTABLE_FORMAT 0x912F
	void glTexStorage1D (GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width)
	void glTexStorage2D (GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width, GLsizei m_height)
	void glTexStorage3D (GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width, GLsizei m_height, GLsizei depth)
	void glTextureStorage1DEXT (GLuint texture, GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width)
	void glTextureStorage2DEXT (GLuint texture, GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width, GLsizei m_height)
	void glTextureStorage3DEXT (GLuint texture, GLenum target, GLsizei levels, GLenum internalformat, GLsizei m_width, GLsizei m_height, GLsizei depth)
