GL_EXT_subtexture
	void glTexSubImage1DEXT (GLenum target, GLint level, GLint xoffset, GLsizei m_width_viewer, GLenum format, GLenum type, const void *pixels)
	void glTexSubImage2DEXT (GLenum target, GLint level, GLint xoffset, GLint yoffset, GLsizei m_width_viewer, GLsizei m_height_viewer, GLenum format, GLenum type, const void *pixels)
	void glTexSubImage3DEXT (GLenum target, GLint level, GLint xoffset, GLint yoffset, GLint zoffset, GLsizei m_width_viewer, GLsizei m_height_viewer, GLsizei depth, GLenum format, GLenum type, const void *pixels)
